<% include ./partials/header %>

<div class="section match">
	<div class="container">
		<div class="row">
			<div class="u-full-width">
			<div class="data">
				<table id="effortsTable" class="items u-full-width">
					<thead>
						<tr>
							<th onclick="sortTable('effortsTable',1,'true');">Date</th>
							<th onclick="sortTable('effortsTable',2,'true');">Item</th>
							<th onclick="sortTable('effortsTable',3,'true');">Effort</th>
						</tr>
					</thead>
					<tbody>
			  			<tr>
			  				<td><%= effort.timestamp %></td>
			  				<td onclick="window.location='/items/<%= effort.item %>'"><%= effort.item %></td>
			  				<td><%= effort.hours %> hour <%= effort.minutes %> mins</td>
						</tr>
					</tbody>
				</table>
			</div>
		</div>
	</div>
</div>

<% include ./partials/footer %>