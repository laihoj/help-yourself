<% include ./partials/header %>

<div class="container">
	<div class="row">
		<div class="data">
			<table id="relevanciesTable" class="categories u-full-width">
				<thead>
					<tr>
						<th onclick="sortTable('relevanciesTable',0,'true');">Label</th>
						<th onclick="sortTable('relevanciesTable',1,'true');">Relevance</th>
					</tr>
				</thead>
				<tbody>
  					<% for(var i = 0; i < relevancies.length; i++) { %>
  					<tr>
						<td 
							style="width:50%"
							onclick="window.location='/categories/<%= relevancies[i].key %>'">
							<%= relevancies[i].label %>
						</td>
						<td style="width:50%">
							<% include ./partials/relevancyslider %>
  						</td>
					<% } %>
					</tr>
				</tbody>
			</table>
		</div>			
	</div>
</div>

<% include ./partials/footer %>


    
    	    
				
				
